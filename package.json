  },
  "homepage": "https://github.com/nearform/fastify-constraints#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "c8": "^10.1.2",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "fastify": "^5.0.0",
    "husky": "^9.0.11",
